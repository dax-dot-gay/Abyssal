layout {
    pane split_direction="vertical" {
        pane {
            command "pnpm"
            args "run" "dev"
            cwd "client/abyssal"
        }
        pane {
            command "cargo"
            args "run" "--" "--config" "abyssal.dev.toml"
            cwd "crates/abyssal"
        }
    }
}